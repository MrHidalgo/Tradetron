// SECTION account profile :: START

section#a-profile.main.a-profile.position-relative

	.main__bg.main__bg-0.position-absolute.w-100.h-100
		img.main__bg-img.main__bg-img-1.position-absolute(src="img/img-main-bg-1.png" srcset="img/img-main-bg-1@2x.png 2x, img/img-main-bg-1@3x.png 3x" alt="" title="")

	.strategy__bg.position-absolute.w-100.h-100
		img(src="img/img-do-bg.png" srcset="img/img-do-bg@2x.png 2x, img/img-do-bg@3x.png 3x" alt="" title="")

	.main__wrapper

		.container
			.row
				.col

					.a-profile__wrapper
						.a-profile__wrapper-left

							// account profile menu
							include _account-profile--menu
							// account profile menu :: end


						.a-profile__wrapper-right
							div

								// account profile
								include _account-profile--profile
								// account profile :: end

								// account profile ledger
								include _account-profile--ledger
								// account profile ledger :: end

								// account profile addFunds
								include _account-profile--addFunds
								// account profile addFunds :: end

								// account profile withdrawFunds
								include _account-profile--withdrawFunds
								// account profile withdrawFunds :: end

								// account profile subscriptions
								include _account-profile--subscriptions
								// account profile subscriptions :: end

								// account profile brokenExchange
								include _account-profile--brokenExchange
								// account profile brokenExchange :: end

								// account profile coupons
								include _account-profile--coupons
								// account profile coupons :: end

								// account profile notifications
								include _account-profile--notifications
								// account profile notifications :: end

								// account profile changePassword
								include _account-profile--changePassword
								// account profile changePassword :: end

	.main__bg.main__bg-1.position-absolute.w-100.h-100
		.main__bg-row.d-flex.position-absolute.w-100.h-100.justify-content-between
			.main__bg-col.main__bg-col-1.position-absolute
			.main__bg-col.main__bg-col-2.d-flex.align-items-start.justify-content-end.position-absolute
				img.main__bg-img.main__bg-img-3.img-fluid.d-block.position-absolute(src="img/img-main-bg-2.png" srcset="img/img-main-bg-2@2x.png 2x, img/img-main-bg-2@3x.png 3x" alt="" title="")


// SECTION account profile :: END
