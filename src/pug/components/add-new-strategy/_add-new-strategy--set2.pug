
.newStrategy__set-wrapper
	
	.newStrategy__set-header.d-flex.align-items-center.justify-content-between
		.newStrategy__set-header-left.d-flex.align-items-center
			p.newStrategy__set-header-title.pr-2!= "Set â„–2"
