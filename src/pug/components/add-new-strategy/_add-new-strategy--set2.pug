		.newStrategy__set-header-right
		a.newStrategy__set-header-close(href="#" class="" title="")
			i.icon-font.icon-close


	// ENTRY CARD
	.newStrategy__section.newStrategy__set.newStrategy__set-entry.px-3.pt-3.pb-4

		.newStrategy__set-head.d-flex.align-items-center.justify-content-between
			.newStrategy__set-head-left.d-flex.align-items-center
				p.newStrategy__set-head-title.pr-3!= "Entry"

				.form-group.mb-0
					select.form-control
						option Select list
						option Select list

				a.newStrategy__set-head-add.ml-3(href="#" class="" title="")!= "+Add"

			.newStrategy__set-head-right.d-flex.align-items-center
				a.newStrategy__set-head-close.mr-2(href="#" class="" title="")
					i.icon-font.icon-close
				a.newStrategy__set-head-corner.ml-2(href="#" class="" title="")
					i.icon-font.icon-corner

		.newStrategy__set-body

			.newStrategy__set-frame.pt-3.d-flex

				.newStrategy__set-frame-left

					.newStrategy__set-frame-title.d-flex.align-items-center.pb-2
						p.pr-3!= "CONDITIONS"
						a(href="#" class="" title="" data-toggle="modal" data-target="#addStrategiesCondition")!= "+Add"

					.newStrategy__set-frame-body.pr-3.pt-3

						table.table.mb-0
							tbody
								tr
									td
										.tabs.d-flex.align-items-center
											a(href="#" class="" title="" data-toggle="modal" data-target="#addStrategiesCondition") Builder
											span.ml-2.mr-2 |
											a(href="#" class="" title="") Advanced Tab
									td

								tr
									td Cond1
									td
										.d-flex.align-items-center.justify-content-end
											a.mr-2(href="#" class="" title="")
												i.icon-font.icon-pencil-edit
											a.ml-2(href="#" class="" title="")
												i.icon-font.icon-garbage
								tr
									td Lorem ipsum
									td
										.d-flex.align-items-center.justify-content-end
											a.mr-2(href="#" class="" title="")
												i.icon-font.icon-pencil-edit
											a.ml-2(href="#" class="" title="")
												i.icon-font.icon-garbage

				.newStrategy__set-frame-right

					.newStrategy__set-frame-title.d-flex.align-items-center.pb-2.pl-3
						p.pr-3!= "positions"
						a(href="#" class="" title="" data-toggle="modal" data-target="#addStrategiesBuilder")!= "+Add"

					.newStrategy__set-frame-body.px-3.pt-3

						.table-responsive
							table.table.mb-0
								tbody
									tr
										td(colspan='6')
											.d-flex.align-items-center.justify-content-between
												span Underlying
												i.pl-2.pr-2 |
												span Strike
												i.pl-2.pr-2 |
												span Type
												i.pl-2.pr-2 |
												span Expiry
												i.pl-2.pr-2 |
												span Qty
												i.pl-2.pr-2 |
												span Price
												i.pl-2.pr-2 |
												span SL
												i.pl-2.pr-2 |
												span Target
									each val, id in [1,2]
										tr
											td Nifty
											td 10900
											td C
											td Feb 26
											td 6000
											td
											td
												.d-flex.align-items-center.justify-content-end
													a.mr-2(href="#" class="" title="")
														i.icon-font.icon-pencil-edit
													a.ml-2(href="#" class="" title="")
														i.icon-font.icon-garbage

			.newStrategy__set-body-footer.pt-4
				a.c-btn.c-btn--border(href="#" class="" title="")!= "Check"
	// ENTRY CARD :: end


