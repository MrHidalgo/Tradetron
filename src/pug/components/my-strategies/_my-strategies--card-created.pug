			span.pb-1!= "ROI"
			p!= "20 %"
		div.d-flex.flex-column
			span.pb-1!= "DD"
			p!= "25 %"
		div.d-flex.flex-column
			span.pb-1!= "Min Capital"
			p!= "Rs.50000"
		div.d-flex.flex-column
			span.pb-1!= "Fees per month"
