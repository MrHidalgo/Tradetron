.myStrategies__section.myStrategies__card.s-profile__description.p-3.position-relative

	.myStrategies__card-badge.d-flex.align-items-center.justify-content-center

		.dropdown.dropdown--more
			button#dropdownUProfileAboutMore.btn.dropdown-toggle.d-flex.align-items-center.justify-content-center.p-0(type='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
				i.icon-font.icon-more
			.dropdown-menu.animate.slideIn(aria-labelledby='dropdownUProfileAboutMore')
				.dropdown-menu-corner.position-absolute
				.dropdown-menu-wrapper.d-flex.flex-column
					a.dropdown-item.d-flex.align-items-center(href='#')
						div
							span!= "Edit"
					a.dropdown-item.d-flex.align-items-center(href='#')
						div
							span!= "Backtest"
					a.dropdown-item.d-flex.align-items-center(href='#')
						div
							span!= "Delete"
					a.dropdown-item.d-flex.align-items-center(href='#')
						div
							span!= "Make private"

	p.s-profile__description-profile-created.d-block.d-md-none.pr-5.pr-md-0.pt-4.pt-md-0!= "created : 08 Apr 2019"

	.s-profile__description-head.pt-2.pt-md-0
		div

			.s-profile__description-profile.d-flex
				div
				div
					div.d-flex.d-md-block
						div
						div
							p.s-profile__description-profile-created.pb-2.d-none.d-md-block!= "created : 08 Apr 2019"
