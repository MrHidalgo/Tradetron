							.s-profile__description-profile-by.d-flex.align-items-center.pb-3
								div
									p!= "by: <a href='#'>Neutrino Advisory Services LLP</a>"

							.s-profile__description-rating.d-flex.d-md-none.flex-column.pb-4
								div
									div.d-flex.align-items-center
										span.position-relative.pr-1
											i.icon-font.icon-star-fill
										span.position-relative.pr-1
											i.icon-font.icon-star-fill
										span.position-relative.pr-1
											i.icon-font.icon-star-fill
										span.position-relative.pr-1
											i.icon-font.icon-star-fill
										span.position-relative
											i.icon-font.icon-star
								div
									span!= "2 ratings"

					div

						.s-profile__description-profile-tags.d-flex.align-items-center.flex-wrap.pb-3
							a.mr-1.mb-1.px-3.py-1(href="#" class="" title="")!= "Bullish"
							a.mr-1.mb-1.px-3.py-1(href="#" class="" title="")!= "Theta Positive"
							a.mr-1.mb-1.px-3.py-1(href="#" class="" title="")!= "Low Gamma"
