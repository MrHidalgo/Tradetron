		div.d-none.d-md-block
			div
				.s-profile__description-rating.d-flex.flex-column.pb-4
					div
						div.d-flex.align-items-center
							span.position-relative.pr-1
								i.icon-font.icon-star-fill
							span.position-relative.pr-1
								i.icon-font.icon-star-fill
							span.position-relative.pr-1
								i.icon-font.icon-star-fill
							span.position-relative.pr-1
								i.icon-font.icon-star-fill
							span.position-relative
								i.icon-font.icon-star
					div
						span!= "2 ratings"
			div
				.s-profile__description-btn.d-flex.flex-column.pb-3
