.subscribers__section.subscribers__self

	.subscribers__self-head.p-3.d-flex.align-items-center.justify-content-between
		div
			p.subscribers__section-title!= "My Subscribers <span>(4)</span>"
		div
		a(href="#" class="" title="")!= "See all"

	.subscribers__self-head.px-3.pb-4.pt-4

		.subscribers__self-block-wrapper

			mixin swiperContent()
